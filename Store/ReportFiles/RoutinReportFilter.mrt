  <Script>using System;
using System.Drawing;
using System.Windows.Forms;
using System.Data;
using Stimulsoft.Controls;
using Stimulsoft.Base.Drawing;
using Stimulsoft.Report;
using Stimulsoft.Report.Dialogs;
using Stimulsoft.Report.Components;

namespace Reports
{
    public class Report : Stimulsoft.Report.StiReport
    {
        public Report()        {
            this.InitializeComponent();
        }
		
		public string DateToshamsi(object date)
		{
			try 
			{	        
				System.Globalization.PersianCalendar shamsi = new System.Globalization.PersianCalendar();

				DateTime da = (DateTime)date;
				int year = shamsi.GetYear(da);
				int month = shamsi.GetMonth(da);
				int day = shamsi.GetDayOfMonth(da);
					return year.ToString()+ "/"+ month.ToString() + "/"+day.ToString()  ;
			}
			catch (Exception)
			{
		
				throw;
			}
		
			return "";

		}
		public string GetNamePresenceStatus(int status)
		{
			try 
			{
				
				if(status==0)
				
				return  "نامشخص"  ;
			   if(status==1)
					return "ورود";
				if(status==2)
					return "خروج";
				if(status==3)
					return "مرخصی روزانه یا استحقاقی";
				if(status==5)
					return "مرخصی استعلاجی";
				if(status==6)
					return "استراحت";
				if(status==7)
					return "ماموریت";
				if(status==8)
					return "شیفت عصر کار ۸ ساعته";
				if(status==9)
					return "مرخصی شیفت عصر کار ۸ ساعته";
				if(status==10)
					return "شیفت شب کار ۸ ساعته";
				if(status==11)
					return "مرخصی شیفت شب کار ۸ ساعته";
				if(status==12)
					return "تعطیلی پنجشنبه";
				else
				return "نامشخص";
			}
			catch (Exception)
			{
		
				throw;
			}
		
			return "";

		}
		public string CurrentDateToshamsi()
		{
			System.Globalization.PersianCalendar shamsi = new System.Globalization.PersianCalendar();

			DateTime da = DateTime.Now;
			int year = shamsi.GetYear(da);
			int month = shamsi.GetMonth(da);
			int day = shamsi.GetDayOfMonth(da);
			return year.ToString()+ "/"+ month.ToString() + "/"+day.ToString() +" " + da.Hour +":"+ da.Minute ;

		}
        #region StiReport Designer generated code - do not modify
		#endregion StiReport Designer generated code - do not modify
    }
}
</Script>
