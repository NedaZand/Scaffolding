@model List<Store.Models.Stores.StoreRoom.InputHasEquipmentModel>

@{
    ViewBag.Title = "  تجهیزات و تعداد تجهیزات خروجی از انبار ";
    Layout = null;

}
<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-12">
        <h4 class="page-title">نمایش لیست تجهیزات ورودی  </h4>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="white-box">
            <div class="table-responsive">
                <div id="example_wrapper" class="dataTables_wrapper">

                    <table id="EquipmentsGrid" class="table table-striped table-condensed table-bordered no-margin nowrap">
                        <thead>
                            <tr>
                                <th class="text-center">عنوان تجهیز</th>
                                <th class="text-center">تعداد وارد شده</th>
                                <th class="text-center">تعداد سالم</th>
                                <th class="text-center">تعداد معیوب</th>
                                <th class="text-center">تعداد مفقود</th>
                                <th class="text-center">تاریخ ورود</th>
                                <th class="text-center">کاربر ایجاد کننده</th>
                                <th class="text-center">تاریخ ایجاد </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td class="text-center">@item.Title</td>
                                    <td class="text-center">@item.Count</td>
                                    <td class="text-center">@item.HealthyNumber</td>
                                    <td class="text-center">@item.DefectiveNumber</td>
                                    <td class="text-center">@item.MissingNumber</td>
                                    <td class="text-center">@item.EntryDate</td>
                                    <td class="text-center">@item.ActionUserName</td>
                                    <td class="text-center">@item.ShamsiCreateDate</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


