    function calculationManpower() {
        $("#material").hide();
        $("#manpower").hide();
        $("#manpowerResult").html('');
        
        var sum = 0;
        var sum1 = 0;
        var sum2 = 0;
        var widths1 = document.getElementById('widthCount').value;
        
        for (var i = 1; i <= widths1; i++) {
            sum1 += parseFloat($("#R" + i + "").val());
        }
        sum1 = Math.ceil(sum1);
        sum1 *= parseInt($("#length").val()) * parseInt($("#height").val());
        
        var widths2 = document.getElementById('widthCount2').value;

        for (var i = 1; i <= widths2; i++) {
            sum2 += parseFloat($("#R2" + i + "").val());
        }
        sum2 = Math.ceil(sum2);
        sum2 *= parseInt($("#length2").val()) * parseInt($("#height2").val());
        
        sum = sum1 + sum2;

        $("#manpowerResult").append('<li class="list-group-item node-treeview6" data-nodeid="0" style="color:undefined;background-color:undefined;"><span class="icon node-icon glyphicon glyphicon-user"></span>&nbsp; نیروی انسانی مورد نیاز<span class="badge">' + Math.ceil(sum / 40) + '</span></li>');

        $("#manpower").slideDown("slow", function () {

        });
    }


