@model Store.Models.Stores.StoreRoom.OutputMaterialModel
@{
    ViewBag.Title = "ثبت تجهیزات خروجی  ";
}



<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-12">
        <h4 class="page-title">فرم ثبت  خروجی جدید</h4>
    </div>
    <div class="col-lg-9 col-md-8 col-12">
      
        <ol class="breadcrumb">
            <li><a href="/Home/Index">داشبورد</a></li>
            <li><a href="/OutputMaterial/Index">نمایش لیست خروجی ها  </a></li>
            <li class="active">ثبت خروجی جدید </li>
        </ol>
    </div>
  
</div>
<div class="row">
    <div class="col-md-12">
        <div class="white-box">
            <div class="alert alert-danger alert-dismissable">
                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                توجه ! تنها دستورکارهایی که داربست به آنها تخصیص داده شده نمایش داده می شوند.
            </div>
            @using (Ajax.BeginForm("Create", "OutputMaterial", null, new AjaxOptions { Url = "/OutputMaterial/Create", HttpMethod = "Post", OnSuccess = "addSuccess" }, new { @enctype = "multipart/form-data", id = "form", @class = "" }))
            {

                @Html.AntiForgeryToken()

                @Html.Partial("_CreateorUpdate", Model)

                <div class="form-actions">
                    <button type="submit" class="btn btn-success btn-rounded"> <i class="fa fa-check"></i> ثبت</button>
                    <a class="btn btn-inverse waves-effect waves-light btn-rounded" href="/OutputMaterial/Index"><i class="fa fa-times"></i> بازگشت</a>

                </div>

            }

        </div>
    </div>
</div>


