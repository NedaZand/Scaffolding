@model Store.Models.Stores.WorkOrderModel
@{
    ViewBag.Title = "مدیریت دستور کار";

}

<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-12">
        <h4 class="page-title">ویرایش دستور کار </h4>
    </div>
    <div class="col-lg-9 col-md-8 col-12">

        <ol class="breadcrumb">
            <li><a href="/Home/Index">داشبورد</a></li>
            <li><a href="/WorkOrder/Index">نمایش دستورکار</a></li>
            <li class="active">ویرایش دستور کار </li>
        </ol>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="white-box">

            @using (Ajax.BeginForm("Edit", "WorkOrder", null, new AjaxOptions { Url = "/WorkOrder/Edit", HttpMethod = "Post", OnSuccess = "editSuccess" }, new { @enctype = "multipart/form-data", id = "form", @class = "" }))
            {
                @Html.HiddenFor(m => m.Id)

                @Html.AntiForgeryToken()

                @Html.Partial("_CreateorUpdate", Model)

                <div class="form-actions">
                    <button type="submit" class="btn btn-success btn-rounded"> <i class="fa fa-check"></i> ویرایش</button>
                    <a class="btn btn-inverse waves-effect waves-light btn-rounded" href="/WorkOrder/Index"><i class="fa fa-times"></i> بازگشت</a>

                </div>

            }

        </div>
    </div>
</div>




