@model Store.Models.Stores.WorkOrderModel
@{
    ViewBag.Title = "مدیریت دستور کار ";
}



<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-12">
        <h4 class="page-title">فرم ثبت  دستور کار جدید</h4>
    </div>
    <div class="col-lg-9 col-md-8 col-12">
       
        <ol class="breadcrumb">
            <li><a href="/Home/Index">داشبورد</a></li>
            <li><a href="/WorkOrder/Index">نمایش دستور کار  </a></li>
            <li class="active">ثبت دستور کار جدید </li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="white-box">
        @using (Ajax.BeginForm("Create", "WorkOrder", null, new AjaxOptions { Url = "/WorkOrder/Create", HttpMethod = "Post", OnSuccess = "addSuccess" }, new { @enctype = "multipart/form-data", id = "form", @class = "" }))
        {

            @Html.AntiForgeryToken()

            @Html.Partial("_CreateorUpdate", Model)

            <div class="form-actions">
                <button type="submit" class="btn btn-success btn-rounded"> <i class="fa fa-check"></i> ثبت</button>

                <a class="btn btn-inverse waves-effect waves-light btn-rounded" href="/WorkOrder/Index"><i class="fa fa-times"></i> بازگشت</a>
            </div>

        }

        </div>
    </div>
</div>

